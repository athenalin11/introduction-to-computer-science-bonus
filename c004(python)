import sys

def main():
    input_data = sys.stdin.read().splitlines()
    t = int(input_data[0])
    for line in input_data[1:t+1]:
        s, d = map(int, line.strip().split())
        if s < d or (s + d) % 2 != 0 or (s - d) % 2 != 0:
            print("impossible")
        else:
            x = (s + d) // 2
            y = (s - d) // 2
            print(f"{x} {y}")

if __name__ == "__main__":
    main()
