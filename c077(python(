def main():
    import sys
    from itertools import combinations

    # 預先建立所有合法字串的編號對應表
    index_map = {}
    count = 1
    # 長度 1 到 5
    for length in range(1, 6):
        # 從 26 個字母中選 length 個，按字典序
        for comb in combinations("abcdefghijklmnopqrstuvwxyz", length):
            word = "".join(comb)
            index_map[word] = count
            count += 1

    # 讀取輸入
    for line in sys.stdin:
        word = line.strip()
        # 檢查是否合法：嚴格遞增
        valid = True
        for i in range(1, len(word)):
            if word[i] <= word[i-1]:
                valid = False
                break
        if valid and word in index_map:
            print(index_map[word])
        else:
            print(0)

if __name__ == "__main__":
    main()
